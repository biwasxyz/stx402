{
  "name": "cloudflare-workers-openapi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "start": "wrangler dev",
    "cf-typegen": "wrangler types",
    "sync-counts": "bun run scripts/sync-endpoint-counts.ts",
    "kv:get": "wrangler kv key get --env-file .env --binding METRICS --text --remote",
    "kv:put": "wrangler kv key put --env-file .env --binding METRICS --remote",
    "cleanup-metrics": "bun run scripts/cleanup-orphaned-metrics.ts",
    "cleanup-metrics:apply": "bun run scripts/cleanup-orphaned-metrics.ts --apply",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@stacks/common": "^7.3.1",
    "@stacks/network": "^7.3.1",
    "@stacks/transactions": "^7.3.1",
    "@stacks/wallet-sdk": "^7.2.0",
    "chanfana": "^3.0.0",
    "hono": "^4.11.3",
    "qrcode": "^1.5.4",
    "x402-stacks": "^1.1.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/service-worker-mock": "^2.0.4",
    "patch-package": "^8.0.1",
    "wrangler": "^4.56.0"
  }
}
